{
    "name" : "rhidja/ezskeleton-bundle",
    "description" : "This bundle add read only custom field type",
    "type" : "symfony-bundle",
    "authors": [
        {
            "name": "Ramtane Hidja",
            "homepage": "https://www.hidja.fr"
        }
    ],
    "require" : {
        "php": ">=7.4",
        "ibexa/core": "~4.6.0",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "autoload" : {
        "psr-4" : {
            "Kabylia\\EzSkeletonBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
